<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2020-11-11T06:05:57Z</date>
    <groups>
        <group>
            <name>Templates/Network devices</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Net ISP Speedtest</template>
            <name>ISP Speedtest</name>
            <description>Monitor internet speed via calls to speedtest.net using speedtest-cli.&#13;
&#13;
Note: speedtest-cli does not guarantee accuracy or consistency with the web based speedtest.net but it is useful for general trending and notification when large changes to internet access speed happen. See the &quot;Inconsistency&quot; notes on the speedtest-cli readme: https://github.com/sivel/speedtest-cli&#13;
&#13;
&#13;
v2.0.2 - 2020-11-10&#13;
&#13;
Maintainer: Rob McCready &lt;rmccready@gmail.com&gt;&#13;
https://github.com/robmccready/zabbix-isp-speedtest</description>
            <groups>
                <group>
                    <name>Templates/Network devices</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Internet Speed</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Speedtest Download</name>
                    <type>DEPENDENT</type>
                    <key>net.isp.speedtest.download</key>
                    <delay>0</delay>
                    <history>1w</history>
                    <trends>750d</trends>
                    <value_type>FLOAT</value_type>
                    <units>b</units>
                    <description>Records download speed (bps)</description>
                    <applications>
                        <application>
                            <name>Internet Speed</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.download</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>net.isp.speedtest.results</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{last({$SPEEDTEST_TRIGGER_AVG_COUNT})}&lt;{$SPEEDTEST_TRIGGER_DOWNLOAD_LOW}</expression>
                            <name>Download Speed LOW</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Speedtest Host</name>
                    <type>DEPENDENT</type>
                    <key>net.isp.speedtest.host</key>
                    <delay>0</delay>
                    <history>1w</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <description>Records the host contacted for the speed test</description>
                    <applications>
                        <application>
                            <name>Internet Speed</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.host</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>net.isp.speedtest.results</key>
                    </master_item>
                </item>
                <item>
                    <name>Speedtest Latency</name>
                    <type>DEPENDENT</type>
                    <key>net.isp.speedtest.latency</key>
                    <delay>0</delay>
                    <history>1w</history>
                    <trends>750d</trends>
                    <value_type>FLOAT</value_type>
                    <units>ms</units>
                    <description>Records the latency to the host used for the speedtest. Not intended to be as accurate as an ICMP test. By default the speedtest cycles between a random set of local servers so the latency may vary with server selection.</description>
                    <applications>
                        <application>
                            <name>Internet Speed</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.latency</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>net.isp.speedtest.results</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{last({$SPEEDTEST_TRIGGER_AVG_COUNT})}&lt;{$SPEEDTEST_TRIGGER_LATENCY_HIGH}</expression>
                            <name>Latency HIGH</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Speedtest Message</name>
                    <type>DEPENDENT</type>
                    <key>net.isp.speedtest.message</key>
                    <delay>0</delay>
                    <history>1w</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <description>Message regarding status of speedtest run</description>
                    <applications>
                        <application>
                            <name>Internet Speed</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.message</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>net.isp.speedtest.results</key>
                    </master_item>
                </item>
                <item>
                    <name>Speedtest Results</name>
                    <type>TRAP</type>
                    <key>net.isp.speedtest.results</key>
                    <delay>0</delay>
                    <history>1w</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <description>Output of a speedtest execution. Actual monitoring happens on the dependent items which pull from the JSON fields</description>
                    <applications>
                        <application>
                            <name>Internet Speed</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Speedtest Success</name>
                    <type>DEPENDENT</type>
                    <key>net.isp.speedtest.success</key>
                    <delay>0</delay>
                    <history>1w</history>
                    <trends>0</trends>
                    <description>Records flag (0=false, 1=true) if speedtest run was successful</description>
                    <applications>
                        <application>
                            <name>Internet Speed</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.success</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>net.isp.speedtest.results</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{last({$SPEEDTEST_TRIGGER_AVG_COUNT})}&lt;&gt;1</expression>
                            <name>Speedtest FAILED</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Speedtest Trigger</name>
                    <key>net.isp.speedtest.trigger[localhost, 10051, {$SPEEDTEST_SENDER_HOST}, {$SPEEDTEST_SERVER_ID}]</key>
                    <delay>{$SPEEDTEST_UPDATE_INTERVAL}</delay>
                    <history>1w</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <description>Scheduled item that triggers a speedtest.</description>
                    <applications>
                        <application>
                            <name>Internet Speed</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Speedtest Upload</name>
                    <type>DEPENDENT</type>
                    <key>net.isp.speedtest.upload</key>
                    <delay>0</delay>
                    <history>1w</history>
                    <trends>750d</trends>
                    <value_type>FLOAT</value_type>
                    <units>b</units>
                    <description>Records upload speed (bps)</description>
                    <applications>
                        <application>
                            <name>Internet Speed</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.upload</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>net.isp.speedtest.results</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{last({$SPEEDTEST_TRIGGER_AVG_COUNT})}&lt;{$SPEEDTEST_TRIGGER_UPLOAD_LOW}</expression>
                            <name>Upload Speed LOW</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
            </items>
            <macros>
                <macro>
                    <macro>{$SPEEDTEST_SENDER_HOST}</macro>
                    <value>Zabbix server</value>
                </macro>
                <macro>
                    <macro>{$SPEEDTEST_SERVER_ID}</macro>
                    <description>Use specific speedtest server (speedtest --server &lt;ID&gt;). When used the trends should be more consistent.</description>
                </macro>
                <macro>
                    <macro>{$SPEEDTEST_TRIGGER_AVG_COUNT}</macro>
                    <value>#3</value>
                    <description>Average last X speedtest runs for trigger values. Avoids alerts on single bad run.</description>
                </macro>
                <macro>
                    <macro>{$SPEEDTEST_TRIGGER_DOWNLOAD_LOW}</macro>
                    <value>100000000</value>
                    <description>Download speed(bps) below which an alert will be raised</description>
                </macro>
                <macro>
                    <macro>{$SPEEDTEST_TRIGGER_LATENCY_HIGH}</macro>
                    <value>50</value>
                    <description>Latency(ms) above which to alert</description>
                </macro>
                <macro>
                    <macro>{$SPEEDTEST_TRIGGER_UPLOAD_LOW}</macro>
                    <value>8000000</value>
                    <description>Upload speed(bps) below which an alert will be raised</description>
                </macro>
                <macro>
                    <macro>{$SPEEDTEST_UPDATE_INTERVAL}</macro>
                    <value>3h</value>
                    <description>How often to execute speedtests</description>
                </macro>
            </macros>
            <screens>
                <screen>
                    <name>ISP Speedtest</name>
                    <hsize>1</hsize>
                    <vsize>3</vsize>
                    <screen_items>
                        <screen_item>
                            <resourcetype>0</resourcetype>
                            <style>0</style>
                            <resource>
                                <name>Speedtest Download</name>
                                <host>Template Net ISP Speedtest</host>
                            </resource>
                            <width>500</width>
                            <height>100</height>
                            <x>0</x>
                            <y>0</y>
                            <colspan>1</colspan>
                            <rowspan>1</rowspan>
                            <elements>0</elements>
                            <valign>0</valign>
                            <halign>0</halign>
                            <dynamic>0</dynamic>
                            <sort_triggers>0</sort_triggers>
                            <url/>
                            <application/>
                            <max_columns>3</max_columns>
                        </screen_item>
                        <screen_item>
                            <resourcetype>0</resourcetype>
                            <style>0</style>
                            <resource>
                                <name>Speedtest Upload</name>
                                <host>Template Net ISP Speedtest</host>
                            </resource>
                            <width>500</width>
                            <height>100</height>
                            <x>0</x>
                            <y>1</y>
                            <colspan>1</colspan>
                            <rowspan>1</rowspan>
                            <elements>0</elements>
                            <valign>0</valign>
                            <halign>0</halign>
                            <dynamic>0</dynamic>
                            <sort_triggers>0</sort_triggers>
                            <url/>
                            <application/>
                            <max_columns>3</max_columns>
                        </screen_item>
                        <screen_item>
                            <resourcetype>0</resourcetype>
                            <style>0</style>
                            <resource>
                                <name>Speedtest Latency</name>
                                <host>Template Net ISP Speedtest</host>
                            </resource>
                            <width>500</width>
                            <height>100</height>
                            <x>0</x>
                            <y>2</y>
                            <colspan>1</colspan>
                            <rowspan>1</rowspan>
                            <elements>0</elements>
                            <valign>0</valign>
                            <halign>0</halign>
                            <dynamic>0</dynamic>
                            <sort_triggers>0</sort_triggers>
                            <url/>
                            <application/>
                            <max_columns>3</max_columns>
                        </screen_item>
                    </screen_items>
                </screen>
            </screens>
        </template>
    </templates>
    <graphs>
        <graph>
            <name>Speedtest Download</name>
            <graph_items>
                <graph_item>
                    <sortorder>1</sortorder>
                    <drawtype>BOLD_LINE</drawtype>
                    <color>90CAF9</color>
                    <item>
                        <host>Template Net ISP Speedtest</host>
                        <key>net.isp.speedtest.download</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
        <graph>
            <name>Speedtest Latency</name>
            <ymin_type_1>FIXED</ymin_type_1>
            <graph_items>
                <graph_item>
                    <sortorder>1</sortorder>
                    <color>E53935</color>
                    <item>
                        <host>Template Net ISP Speedtest</host>
                        <key>net.isp.speedtest.latency</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
        <graph>
            <name>Speedtest Summary</name>
            <show_work_period>NO</show_work_period>
            <show_triggers>NO</show_triggers>
            <graph_items>
                <graph_item>
                    <sortorder>1</sortorder>
                    <drawtype>FILLED_REGION</drawtype>
                    <color>90CAF9</color>
                    <yaxisside>RIGHT</yaxisside>
                    <item>
                        <host>Template Net ISP Speedtest</host>
                        <key>net.isp.speedtest.download</key>
                    </item>
                </graph_item>
                <graph_item>
                    <sortorder>2</sortorder>
                    <drawtype>FILLED_REGION</drawtype>
                    <color>1565C0</color>
                    <yaxisside>RIGHT</yaxisside>
                    <item>
                        <host>Template Net ISP Speedtest</host>
                        <key>net.isp.speedtest.upload</key>
                    </item>
                </graph_item>
                <graph_item>
                    <sortorder>3</sortorder>
                    <color>E53935</color>
                    <item>
                        <host>Template Net ISP Speedtest</host>
                        <key>net.isp.speedtest.latency</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
        <graph>
            <name>Speedtest Upload</name>
            <graph_items>
                <graph_item>
                    <sortorder>1</sortorder>
                    <drawtype>BOLD_LINE</drawtype>
                    <color>1565C0</color>
                    <item>
                        <host>Template Net ISP Speedtest</host>
                        <key>net.isp.speedtest.upload</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
    </graphs>
</zabbix_export>
